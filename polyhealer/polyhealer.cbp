		</Unit>
		<Unit filename="polysplit.h">
			<Option virtualFolder="healing/" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
