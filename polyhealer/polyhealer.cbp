		</Unit>
		<Unit filename="polyhealer.h">
			<Option virtualFolder="healing/" />
		</Unit>
		<Unit filename="polyhealer_config.h" />
		<Unit filename="polyremesh.cpp">
			<Option virtualFolder="remesh/" />
		</Unit>
		<Unit filename="polyremesh.h">
			<Option virtualFolder="remesh/" />
		</Unit>
		<Unit filename="polysplit.cpp">
